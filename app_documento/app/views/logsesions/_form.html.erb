<%= semantic_form_for @logsesion do |f| %>
  <%= f.inputs do %>
    <%= f.input :usuario_id %>
    <%= f.input :super_id %>
    <%= f.input :tipo %>
    <%= f.input :nusuario %>
    <%= f.input :nsuper %>
  <% end %>

  <%= f.actions do %>
    <%= f.action :submit, :as => :input %>
  <% end %>
<% end %>
